#!/usr/bin/env bash

#Personal stuff
alias cd..='cd ..'
alias cd.2='cd ../..'
alias cd.3='cd ../../..'
alias cd.4='cd ../../../..'
alias cd.5='cd ../../../../..'
alias cddev='cd ~/dev'

alias CD='cd'
alias cgrep="grep --color=always"

##Â Git
alias gstat="git status"
alias gstash="git stash"
alias gcommit="git commit"
alias gbranch="git branch"
alias gpull="git pull"
alias gpullo="git pull origin"
alias gpush="git push"
alias gco="git checkout"
alias glog="git log"
alias gyday="git log --author=Fabian --since=yesterday"

if [[ $df_OS_UNIX = true ]]; then
	alias subl="sublime"
else
	alias sublime="sublime_text"
fi
